select customer.city as c0, customer.gender as c1, customer.address2 as c2, sum(sales_fact_1997.unit_sales) as m0 from customer as customer, sales_fact_1997 as sales_fact_1997 where sales_fact_1997.customer_id = customer.customer_id and customer.city = 'Redwood City' and (customer.address2 in ('#2', '#208', 'Unit H103') or customer.address2 is null) group by customer.city, customer.gender, customer.address2;
