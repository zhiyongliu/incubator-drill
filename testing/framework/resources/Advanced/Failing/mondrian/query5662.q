select time_by_day.the_year as c0, alt_promotion.promo_id as c1, sum(sales_fact_1997.unit_sales) as m0 from time_by_day as time_by_day, sales_fact_1997 as sales_fact_1997, (SELECT * FROM (VALUES (0, 'First promo'), (1, 'Second promo')) AS t (promo_id, promo_name)) as alt_promotion where sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and sales_fact_1997.promotion_id = alt_promotion.promo_id group by time_by_day.the_year, alt_promotion.promo_id;
