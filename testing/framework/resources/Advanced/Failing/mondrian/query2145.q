select store.store_country as c0, store.store_state as c1, customer.country as c2, customer.state_province as c3, customer.city as c4, customer.customer_id as c5, fullname as c6, fullname as c7, customer.gender as c8, customer.marital_status as c9, customer.education as c10, customer.yearly_income as c11 from store as store, sales_fact_1997 as sales_fact_1997, customer as customer where sales_fact_1997.store_id = store.store_id and sales_fact_1997.customer_id = customer.customer_id and (store.store_state in ('CA', 'WA')) and (customer.customer_id in (570, 7851, 6442)) group by store.store_country, store.store_state, customer.country, customer.state_province, customer.city, customer.customer_id, fullname, customer.gender, customer.marital_status, customer.education, customer.yearly_income order by store.store_country ASC NULLS LAST, store.store_state ASC NULLS LAST, customer.country ASC NULLS LAST, customer.state_province ASC NULLS LAST, customer.city ASC NULLS LAST, fullname ASC NULLS LAST;
