select * from (select product_class.product_family as c0, store.store_country as c1 from product as product, product_class as product_class, inventory_fact_1997 as inventory_fact_1997, store as store where product.product_class_id = product_class.product_class_id and inventory_fact_1997.product_id = product.product_id and inventory_fact_1997.store_id = store.store_id group by product_class.product_family, store.store_country union select product_class.product_family as c0, store.store_country as c1 from product as product, product_class as product_class, sales_fact_1997 as sales_fact_1997, store as store where product.product_class_id = product_class.product_class_id and sales_fact_1997.product_id = product.product_id and sales_fact_1997.store_id = store.store_id group by product_class.product_family, store.store_country) as unionQuery order by 1 ASC NULLS LAST, 2 ASC NULLS LAST;
