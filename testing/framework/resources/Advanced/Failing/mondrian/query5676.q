select alt_promotion.promo_id as c0, alt_promotion.promo_name as c1 from (SELECT * FROM (VALUES (0, CAST(NULL AS VARCHAR(6))), (1, 'Promo1')) AS t (promo_id, promo_name)) as alt_promotion where UPPER(alt_promotion.promo_name) = UPPER('Promo1') group by alt_promotion.promo_id, alt_promotion.promo_name order by alt_promotion.promo_id ASC NULLS LAST;
