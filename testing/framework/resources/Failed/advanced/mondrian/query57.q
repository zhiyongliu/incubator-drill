select time_by_day.the_year as c0, sn.desc as c1, sum(sales_fact_1997.unit_sales) as m0 from time_by_day as time_by_day, sales_fact_1997 as sales_fact_1997, (SELECT * FROM (VALUES (1, 'SameName')) AS t (id, desc)) as sn where sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and sales_fact_1997.customer_id = sn.id and sn.desc = 'SameName' group by time_by_day.the_year, sn.desc;
