select time_by_day.the_year as `Year`, time_by_day.quarter as Quarter, product_class.product_family as Product Family, customer.gender as Gender, sales_fact_1997.store_sales as Store Sales from time_by_day as time_by_day, sales_fact_1997 as sales_fact_1997, product_class as product_class, product as product, customer as customer where sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and time_by_day.quarter = 'Q2' and sales_fact_1997.product_id = product.product_id and product.product_class_id = product_class.product_class_id and product_class.product_family = 'Non-Consumable' and sales_fact_1997.customer_id = customer.customer_id and customer.gender = 'M' order by time_by_day.the_year ASC, time_by_day.quarter ASC, product_class.product_family ASC, customer.gender ASC;
