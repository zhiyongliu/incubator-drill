select cast(row_key as integer) voter_id, convert_from(t.onecf.name, 'UTF8') name, cast(t.twocf.age as integer) age, cast(t.twocf.registration as varchar(20)) registration, cast(t.threecf.contributions as decimal(6,2)) contributions, cast(t.threecf.voterzone as integer) voterzone,cast(t.fourcf.create_date as timestamp) create_date from m7voter t where t.onecf.name not similar to '%(young|u|a|i|e|m)%';
